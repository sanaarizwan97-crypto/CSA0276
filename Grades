#include <stdio.h>

int main() {
    int marks;

    // First mark
    marks = 95;
    if (marks >= 90)
        printf("Marks: %d-Grade: A\n", marks);
    else if (marks >= 60)
        printf("Marks: %d-Grade: B\n", marks);
    else if (marks >= 50)
        printf("Marks: %d-Grade: C\n", marks);
    else
        printf("Marks: %d-Grade: Fail\n", marks);

    // Second mark
    marks = 65;
    if (marks >= 90)
        printf("Marks: %d-Grade: A\n", marks);
    else if (marks >= 60)
        printf("Marks: %d-Grade: B\n", marks);
    else if (marks >= 50)
        printf("Marks: %d-Grade: C\n", marks);
    else
        printf("Marks: %d-Grade: Fail\n", marks);

    // Third mark
    marks = 35;
    if (marks >= 90)
        printf("Marks: %d-Grade: A\n", marks);
    else if (marks >= 60)
        printf("Marks: %d-Grade: B\n", marks);
    else if (marks >= 50)
        printf("Marks: %d-Grade: C\n", marks);
    else
        printf("Marks: %d-Grade: Fail\n", marks);

    return 0;
}
